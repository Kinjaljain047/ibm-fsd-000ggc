<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
 <display-name>Archetype Created Web Application</display-name>
 <servlet>
  <servlet-name>ControllerServlet</servlet-name>
  <display-name>ControllerServlet</display-name>
  <servlet-class>com.controller.ControllerServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>InternationalController</servlet-name>
  <display-name>InternationalController</display-name>
  <servlet-class>com.controller.InternationalController</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>SpecialEdition</servlet-name>
  <display-name>SpecialEdition</display-name>
  <servlet-class>com.controller.SpecialEdition</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>ControllerServlet</servlet-name>
  <url-pattern>/ControllerServlet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>InternationalController</servlet-name>
  <url-pattern>/inter.controller</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>SpecialEdition</servlet-name>
  <url-pattern>/SpecialEdition</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>/cddetails.jsp</welcome-file>
 </welcome-file-list>
</web-app>
